#B58858! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

!----------------------------------------------------------------------------!
!									     !
!	U R X V T - Settings						     !
!									     !
!----------------------------------------------------------------------------!
URxvt*geometry: 80x23
URxvt*internalBorder: 2
!URxvt*borderColor: #151515
!URxvt*borderLess:				true
URxvt*depth:					32
URxvt*scrollBar_right:				true
URxvt*font:					xft:lime:pixelsize=8:antialias=true:hinting=true
URxvt*boldFont: 			xft:lime:pixelsize=8:antialias=true:hinting:true
URxvt*cursorBlink:				true
URxvt*cursorUnderline:				true
URxvt*scrollColor:				#111111
URxvt*perl-ext-common:				default,clipboard,matcher
urxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt.scrollBar:				off
URxvt*buffered: true
URxvt.tabbed.tabbar-fg:			 	7
URxvt.tabbed.tabbar-bg: 			0
URxvt.tabbed.tab-fg:    			4
URxvt.tabbed.tab-bg:    			0	
URxvt*termName:				 	rxvt-256color
URxvt.urlLauncher:				/usr/bin/firefox
URxvt.matcher.button:				1


!-------------------------------------------------------------------------!
!									  !
!	U R X V T - Colors						  !
!									  !
!-------------------------------------------------------------------------!

URxvt*colorBD: #ffffff
URxvt*colorIT: #a761c2
URxvt*colorUL: #6d9cbe
URxvt*underlineColor: #435D75
URxvt*cursorColor:       #435D75

URxvt*highlightColor: #435D75
URxvt*highlightTextColor: #90EE90

URxvt.background: #232323
